{
  "expo": {
    "name": "HealthcareApp",
    "slug": "HealthcareApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/heart-rate.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/heart-rate.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ailege.healthcareapp",
      "newArchEnabled": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/heart-rate.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ailege.healthcareapp",
      "newArchEnabled": true
    },
    "web": {
      "favicon": "./assets/heart-rate.png"
    },
    "plugins": [
      "expo-build-properties"
    ]
  }
}